<h1>投稿された場所一覧</h1>

<div id="place" class="clearfix">
  <% @places.each do |place| %>
    <ul>
      <li>
        <p class="top_image"><%= link_to place.id, place %> : <%= place.adress %></p>

      </li>
    </ul>
  <% end %>
</div>


<div>
   <%= link_to '新しい場所に写真を投稿する', new_place_path, class: 'btn btn-primary btn-block' %>
</div>